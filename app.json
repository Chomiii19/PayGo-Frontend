{
  "expo": {
    "name": "PayGo",
    "slug": "paygo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "scheme": "paygo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#101010"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.paygo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon.png",
        "backgroundColor": "#101010"
      },
      "package": "com.anonymous.paygo"
    },
    "web": {
      "favicon": "./assets/icons/icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/icon.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#101010"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a1591f57-fc05-4984-ab60-d73bb1435e91"
      }
    }
  }
}
